# Add your API keys here
# Copy from .env.example and add your actual keys

# GitHub API (Optional - for higher rate limits)
REACT_APP_GITHUB_TOKEN=your_github_personal_access_token_here

# Cohere API Key (Required for AI features)
REACT_APP_COHERE_API_KEY=your_cohere_api_key_here

REACT_APP_GITHUB_CLIENT_ID=your_github_client_id_here
# Dynamic redirect URI - will be determined at runtime
REACT_APP_REDIRECT_URI=https://github-insights-pi.vercel.app/__/auth/handler
# REACT_APP_REDIRECT_URI=http://localhost:3000/__/auth/handler  
# Uncomment this line for local development
# Make sure to switch back to the production URI when deploying
# Also ensure that the redirect URIs are set correctly in your GitHub OAuth app settings